{
  "name": "booksquare-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.isLocalhost next dev -p 9000",
    "start:staging": "env-cmd -f .env.isStaging next dev -H staging -p 9000",
    "start:production": "env-cmd -f .env.isProduction next dev -H production -p 9000",
    "build": "next build",
    "build:staging": "env-cmd -f .env.isStaging next build",
    "build:production": "env-cmd -f .env.isProduction next build",
    "serve": "env-cmd -f .env.isLocalhost next start -p 9090",
    "serve:staging": "env-cmd -f .env.isStaging next start -p 9090",
    "serve:production": "env-cmd -f .env.isProduction next start -p 9090",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:run:staging": "./node_modules/.bin/cypress run",
    "cypress:start": "start-server-and-test start http://localhost:9000 cypress:open",
    "cypress:start:staging": "CYPRESS_BASE_URL=http://staging:9000 start-server-and-test start:staging http://staging:9000 cypress:open",
    "cypress:ci": "env-cmd -f .env.isLocalhost start-server-and-test start http://localhost:9000 cypress:run",
    "cypress:ci:staging": "env-cmd -f .env.isStaging start-server-and-test start http://localhost:9000 cypress:run:staging",
    "prettier:report": "prettier '**/*.{js,tsx,ts,scss,json,md}' --check",
    "prettier:fix": "prettier '**/*.{js,tsx,ts,scss,json,md}' --write",
    "eslint:report": "eslint '**/*.{js,tsx,ts}' --cache",
    "eslint:fix": "eslint '**/*.{js,tsx,ts}' --fix",
    "stylelint:report": "stylelint '**/*.scss' --cache",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "typescript:report": "tsc --noEmit && tsc --noEmit --project cypress/tsconfig.json",
    "graphql:operations": "amplify-graphql-docs-generator --schema graphql/graphql.schema.json --maxDepth=4 --output src/graphql/operations.graphql --language graphql",
    "graphql:codegen": "graphql-codegen --config graphql/.graphqlrc.yml",
    "graphql:codegen:hooks": "graphql-codegen --config graphql/.graphqlrc-operations.yml",
    "graphql:schema": "graphql-introspection-json-to-sdl graphql/graphql.schema.json > graphql/graphql.schema.json.graphql",
    "graphql:generate": "npm-run-all graphql:codegen graphql:operations graphql:codegen:hooks graphql:schema",
    "lang:extract": "formatjs extract 'src/**/*.{js,tsx,ts}' --out-file src/lang/cli/extracted.json --ignore **/*.d.ts",
    "lang:compile": "formatjs compile src/lang/cli/extracted.json --out-file src/lang/cli/compiled.json"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/browser": "^7.37.2",
    "@sentry/nextjs": "7.21.1",
    "@sentry/tracing": "7.21.1",
    "@stripe/react-stripe-js": "1.4.1",
    "@stripe/stripe-js": "1.15.0",
    "@types/js-sha512": "^0.8.0",
    "@types/mixpanel-browser": "^2.38.1",
    "@types/pdfjs-dist": "2.10.377",
    "@types/react-lottie": "^1.2.6",
    "@types/react-text-truncate": "^0.14.1",
    "apollo-upload-client": "^16.0.0",
    "babel-eslint": "^10.1.0",
    "date-fns": "^2.28.0",
    "epubjs": "^0.3.93",
    "framer-motion": "^6.2.7",
    "graphql": "15.5.0",
    "graphql-cli": "4.1.0",
    "iconsax-react": "^0.0.8",
    "js-sha512": "^0.8.0",
    "mixpanel-browser": "^2.45.0",
    "next": "^13.1.6",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "2.10.377",
    "react": "^18.2.0",
    "react-apple-login": "^1.1.3",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-gtm-module": "^2.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^6.13.1",
    "react-intl": "^5.24.6",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.6",
    "react-select": "^4.3.1",
    "react-text-truncate": "^0.19.0",
    "react-textarea-autosize": "^8.3.3",
    "react-use-intercom": "^1.5.1",
