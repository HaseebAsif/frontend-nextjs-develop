    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sass": "^1.49.8",
    "sharp": "^0.30.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "^1.2.0",
    "tabbable": "^5.2.1",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
